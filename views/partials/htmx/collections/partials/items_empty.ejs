<div id="collections-items-list-row" class="col-8 m-0 p-0"
    <% if (typeof endpointUrl === "string" && endpointUrl.length) { %>
        hx-get="<%= endpointUrl %>"
        hx-trigger="load"
        hx-swap="outerHTML"
    <% } else if (typeof baseUrl !== "undefined") { %>
        <%- include("../items/update-items-list-signal.ejs", {
            baseUrl,
            itemSearchTerm: typeof searchTerm === "string" ? searchTerm : undefined,
        }) %>
    <% } %>
>
    <div class="card h-100 border-end-0 rounded-0">
        <%- include("../items/items_header_bar.ejs") %>
        <div class="card-body d-flex justify-content-center align-items-center">
            <div>No Items</div>
        </div>
    </div>
</div>
